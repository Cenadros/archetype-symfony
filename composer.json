{
    "name": "runroom/symfony_archetype",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "a2lix/translation-form-bundle": "2.4.1",
        "doctrine/doctrine-bundle": "1.9.1",
        "doctrine/doctrine-migrations-bundle": "1.3.1",
        "doctrine/orm": "2.6.2",
        "friendsofsymfony/ckeditor-bundle": "1.2.0",
        "friendsofsymfony/user-bundle": "2.1.2",
        "gedmo/doctrine-extensions": "2.4.36",
        "incenteev/composer-parameter-handler": "2.1.3",
        "knplabs/doctrine-behaviors": "1.5.0",
        "pixassociates/sortable-behavior-bundle": "1.5.0",
        "sonata-project/admin-bundle": "3.40.2",
        "sonata-project/block-bundle": "3.12.1",
        "sonata-project/core-bundle": "3.11.2",
        "sonata-project/doctrine-orm-admin-bundle": "3.6.2",
        "sonata-project/easy-extends-bundle": "2.5.0",
        "sonata-project/formatter-bundle": "4.1.2",
        "sonata-project/media-bundle": "3.16.2",
        "sonata-project/user-bundle": "4.2.3",
        "stof/doctrine-extensions-bundle": "1.3.0",
        "symfony/framework-bundle": "3.4.17",
        "symfony/lts": "3.0",
        "symfony/monolog-bundle": "3.3.0",
        "symfony/security-bundle": "3.4.17",
        "symfony/swiftmailer-bundle": "3.2.3",
        "symfony/twig-bundle": "3.4.17",
        "twig/extensions": "1.5.2",
        "twig/twig": "2.5.0"
    },
    "require-dev": {
        "deployer/dist": "4.3.4",
        "doctrine/doctrine-fixtures-bundle": "3.0.2",
        "friendsofphp/php-cs-fixer": "2.13.0",
        "phpstan/phpstan-shim": "0.10.3",
        "phpunit/dbunit": "4.0.0",
        "phpunit/phpunit": "7.4.1",
        "sensio/generator-bundle": "3.1.7",
        "symfony/debug-bundle": "3.4.17",
        "symfony/web-profiler-bundle": "3.4.17"
    },
    "scripts": {
        "build-parameters": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "app-scripts": [
            "@php bin/console ckeditor:install --clear=skip --release=full --tag=4.8.0 --quiet",
            "@php bin/console assets:install --relative"
        ],
        "post-install-cmd": [
            "@build-parameters",
            "@app-scripts"
        ],
        "post-update-cmd": [
            "@build-parameters",
            "@app-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
