{
    "name": "runroom/symfony_archetype",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3",
        "a2lix/translation-form-bundle": "2.4.3",
        "doctrine/doctrine-bundle": "1.10.2",
        "doctrine/doctrine-migrations-bundle": "2.0.0",
        "doctrine/orm": "2.6.3",
        "friendsofsymfony/ckeditor-bundle": "1.2.0",
        "friendsofsymfony/user-bundle": "2.1.2",
        "gedmo/doctrine-extensions": "2.4.37",
        "incenteev/composer-parameter-handler": "2.1.3",
        "knplabs/doctrine-behaviors": "1.6.0",
        "pixassociates/sortable-behavior-bundle": "1.5.0",
        "sonata-project/admin-bundle": "3.47.0",
        "sonata-project/block-bundle": "3.15.0",
        "sonata-project/core-bundle": "3.16.2",
        "sonata-project/doctrine-orm-admin-bundle": "3.8.3",
        "sonata-project/easy-extends-bundle": "2.5.0",
        "sonata-project/formatter-bundle": "4.1.2",
        "sonata-project/media-bundle": "3.19.1",
        "sonata-project/user-bundle": "4.3.0",
        "stof/doctrine-extensions-bundle": "1.3.0",
        "symfony/framework-bundle": "3.4.23",
        "symfony/monolog-bundle": "3.3.1",
        "symfony/security-bundle": "3.4.23",
        "symfony/swiftmailer-bundle": "3.2.5",
        "symfony/twig-bundle": "3.4.23",
        "twig/extensions": "1.5.4",
        "twig/twig": "2.7.4"
    },
    "require-dev": {
        "deployer/dist": "4.3.4",
        "doctrine/doctrine-fixtures-bundle": "3.1.0",
        "friendsofphp/php-cs-fixer": "2.14.2",
        "phpstan/phpstan-shim": "0.11.5",
        "phpunit/phpunit": "8.0.6",
        "symfony/debug-bundle": "4.2.4",
        "symfony/maker-bundle": "1.11.5",
        "symfony/web-profiler-bundle": "3.4.23",
        "theofidry/alice-data-fixtures": "1.1.1"
    },
    "scripts": {
        "build-parameters": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "app-scripts": [
            "@php bin/console ckeditor:install --clear=skip --release=full --tag=4.8.0 --quiet",
            "@php bin/console assets:install --relative"
        ],
        "post-install-cmd": [
            "@build-parameters",
            "@app-scripts"
        ],
        "post-update-cmd": [
            "@build-parameters",
            "@app-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
