{
    "name": "runroom/symfony_archetype",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "exclude-from-classmap": [
            "/src/Migrations/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3",
        "a2lix/translation-form-bundle": "2.4.3",
        "doctrine/doctrine-bundle": "1.12.0",
        "doctrine/doctrine-migrations-bundle": "2.1.2",
        "doctrine/orm": "2.7.0",
        "friendsofsymfony/ckeditor-bundle": "1.2.0",
        "friendsofsymfony/user-bundle": "2.1.2",
        "gedmo/doctrine-extensions": "2.4.37",
        "incenteev/composer-parameter-handler": "2.1.3",
        "jaybizzle/crawler-detect": "1.2.89",
        "knplabs/doctrine-behaviors": "1.6.0",
        "runroom/sortable-behavior-bundle": "1.0.1",
        "runroom/redirection-bundle": "1.0.0",
        "runroom/translation-bundle": "1.0.0",
        "sonata-project/admin-bundle": "3.54.1",
        "sonata-project/block-bundle": "3.18.2",
        "sonata-project/core-bundle": "3.x-dev",
        "sonata-project/doctrine-orm-admin-bundle": "3.11.0",
        "sonata-project/easy-extends-bundle": "2.5.0",
        "sonata-project/formatter-bundle": "4.1.3",
        "sonata-project/media-bundle": "3.21.0",
        "sonata-project/user-bundle": "4.5.1",
        "stof/doctrine-extensions-bundle": "1.3.0",
        "symfony/framework-bundle": "4.4.0",
        "symfony/monolog-bundle": "3.5.0",
        "symfony/security-bundle": "4.4.0",
        "symfony/swiftmailer-bundle": "3.4.0",
        "symfony/twig-bundle": "4.4.0",
        "twig/extensions": "1.5.4",
        "twig/twig": "2.12.2"
    },
    "require-dev": {
        "deployer/dist": "4.3.4",
        "doctrine/doctrine-fixtures-bundle": "3.3.0",
        "friendsofphp/php-cs-fixer": "2.16.0",
        "phpstan/phpstan-shim": "0.11.19",
        "phpunit/phpunit": "8.4.3",
        "symfony/debug-bundle": "4.4.0",
        "symfony/maker-bundle": "1.14.3",
        "symfony/web-profiler-bundle": "4.4.0",
        "theofidry/alice-data-fixtures": "1.1.1"
    },
    "scripts": {
        "build-parameters": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "app-scripts": [
            "@php bin/console ckeditor:install --clear=skip --release=full --tag=4.13.0 --quiet",
            "@php bin/console assets:install --relative"
        ],
        "post-install-cmd": [
            "@build-parameters",
            "@app-scripts"
        ],
        "post-update-cmd": [
            "@build-parameters",
            "@app-scripts"
        ]
    },
    "extra": {
        "incenteev-parameters": {
            "file": "config/parameters.yaml"
        }
    }
}
