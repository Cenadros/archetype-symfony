{
    "name": "runroom/symfony_archetype",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
        "php": "^5.6",
        "symfony/symfony": "2.8.8",
        "doctrine/orm": "2.5.4",
        "doctrine/doctrine-bundle": "1.6.3",
        "symfony/swiftmailer-bundle": "2.3.11",
        "symfony/monolog-bundle": "2.11.1",
        "sensio/distribution-bundle": "5.0.7",
        "sensio/framework-extra-bundle": "3.0.16",
        "incenteev/composer-parameter-handler": "2.1.2",

        "sensio/generator-bundle": "3.0.7",
        "doctrine/doctrine-migrations-bundle": "1.1.1",
        "doctrine/doctrine-fixtures-bundle": "2.3.0",
        "knplabs/doctrine-behaviors": "1.3.2",
        "knplabs/knp-menu-bundle": "2.1.2",
        "friendsofsymfony/user-bundle": "1.3.6",
        "a2lix/translation-form-bundle": "2.1.2",
        "egeloen/ckeditor-bundle": "4.0.3",
        "stof/doctrine-extensions-bundle": "1.2.2",
        "gedmo/doctrine-extensions": "2.4.18",
        "pixassociates/sortable-behavior-bundle": "1.0",
        "twig/extensions": "1.3.0",
        "jms/serializer-bundle": "1.1.0",

        "sonata-project/media-bundle": "3.1.0",
        "sonata-project/classification-bundle": "3.1.0",
        "sonata-project/core-bundle": "3.0.3",
        "sonata-project/admin-bundle": "3.4.0",
        "sonata-project/doctrine-orm-admin-bundle": "3.0.5",
        "sonata-project/user-bundle": "3.0.1",
        "sonata-project/block-bundle": "3.1.1",
        "sonata-project/formatter-bundle": "3.0.1",
        "sonata-project/easy-extends-bundle": "2.1.10",
        "sonata-project/intl-bundle": "2.2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.7",
        "phpunit/dbunit": "2.0.2",
        "friendsofphp/php-cs-fixer": "2.0.0-alpha"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
