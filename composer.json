{
    "name": "runroom/gpcasinos_web",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
        "php": "~5.5",
        "symfony/symfony": "2.8.4",
        "doctrine/orm": "2.5.4",
        "doctrine/doctrine-bundle": "1.6.2",
        "symfony/swiftmailer-bundle": "2.3.11",
        "symfony/monolog-bundle": "2.10.0",
        "sensio/distribution-bundle": "5.0.5",
        "sensio/framework-extra-bundle": "3.0.16",
        "incenteev/composer-parameter-handler": "2.1.2",

        "sensio/generator-bundle": "3.0.6",
        "doctrine/doctrine-migrations-bundle": "1.1.1",
        "knplabs/doctrine-behaviors": "1.3.2",
        "friendsofsymfony/user-bundle": "1.3.6",
        "a2lix/translation-form-bundle": "2.1.1",
        "sonata-project/admin-bundle": "dev-master",
        "sonata-project/doctrine-orm-admin-bundle": "dev-master",
        "sonata-project/user-bundle": "dev-master",
        "sonata-project/easy-extends-bundle": "2.1.10",
        "sonata-project/intl-bundle": "2.2.4",
        "knplabs/knp-menu-bundle": "2.1.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "2.8.4",

        "phpunit/phpunit": "4.8.24",
        "phpunit/dbunit": "2.0.2",
        "doctrine/doctrine-fixtures-bundle": "2.3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
